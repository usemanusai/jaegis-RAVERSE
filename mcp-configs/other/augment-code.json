{
  "mcpServers": {
    "raverse": {
      "command": "npx",
      "args": ["-y", "raverse-mcp-server@1.0.13"],
      "env": {
        "PROXY_URL": "https://raverse-mcp-proxy.use-manus-ai.workers.dev",
        "BACKEND_URL": "https://jaegis-raverse.onrender.com",
        "LOG_LEVEL": "INFO",
        "SERVER_VERSION": "1.0.13",

        "REDIS_URL": "rediss://default:REDACTED@raverse-valkey-cache-raverse-pg-db.g.aivencloud.com:23056",
        "REDIS_HOST": "raverse-valkey-cache-raverse-pg-db.g.aivencloud.com",
        "REDIS_PORT": "23056",
        "REDIS_USER": "default",
        "REDIS_PASSWORD": "REDACTED",
        "REDIS_TLS": "true",

        "DATABASE_URL": "postgres://avnadmin:REDACTED@raverse-pg-db-raverse-pg-db.i.aivencloud.com:23055/defaultdb?sslmode=require",
        "POSTGRES_HOST": "raverse-pg-db-raverse-pg-db.i.aivencloud.com",
        "POSTGRES_PORT": "23055",
        "POSTGRES_USER": "avnadmin",
        "POSTGRES_PASSWORD": "REDACTED",
        "POSTGRES_DB": "defaultdb",
        "POSTGRES_SSLMODE": "require",
        "POSTGRES_CA_CERT": ""
      }
    },
    "raverse-mcp-proxy": {
      "disabled": true,
      "command": "npx",
      "args": ["-y", "raverse-mcp-proxy@latest"],
      "env": {
        "PROXY_URL": "https://raverse-mcp-proxy.use-manus-ai.workers.dev",
        "BACKEND_URL": "https://jaegis-raverse.onrender.com"
      }
    }
  }
}

