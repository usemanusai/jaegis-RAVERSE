{
  "name": "raverse-mcp-proxy",
  "version": "1.0.0",
  "description": "Cloudflare Worker proxy for RAVERSE MCP server with edge caching and request routing",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "wrangler dev",
    "dev": "wrangler dev --local",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint .",
    "format": "prettier --write .",
    "health-check": "curl https://raverse-mcp-proxy.use-manus-ai.workers.dev/health",
    "logs": "wrangler tail",
    "logs:live": "wrangler tail --follow",
    "setup": "npm install && wrangler login",
    "verify": "node verify-deployment.js"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "proxy",
    "raverse",
    "mcp",
    "edge-computing",
    "caching"
  ],
  "author": "RAVERSE Team",
  "license": "MIT",
  "dependencies": {
    "wrangler": "^4.45.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250101.0",
    "@eslint/js": "^9.0.0",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@rollup/plugin-inject": "^5.0.0",
    "eslint": "^9.0.0",
    "glob": "^10.0.0",
    "jest": "^29.7.0",
    "lru-cache": "^10.0.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usemanusai/jaegis-RAVERSE.git",
    "directory": "raverse-mcp-proxy"
  },
  "bugs": {
    "url": "https://github.com/usemanusai/jaegis-RAVERSE/issues"
  },
  "homepage": "https://github.com/usemanusai/jaegis-RAVERSE#readme"
}
